#include<iostream>

using namespace std;

class FlyingKoopa {
private:
	int xPostion;
	int yPostion;
	char dir;
public:
	FlyingKoopa(int x, int y);
	void fly();
	void print();

};

int main() {
	FlyingKoopa g1(200, 200);
	while (1) {
		g1.fly();
		g1.print();
	}

}

FlyingKoopa::FlyingKoopa(int x, int y) {
	xPostition = x;
	yPosition = y;
	dir = 'l';
}
